    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.5",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "postcss": "^8",
    "prettier": "^3.4.2",
